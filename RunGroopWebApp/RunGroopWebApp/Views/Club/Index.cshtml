@model IEnumerable<Club>

@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

<div class="container">

    <div class="row">
        @foreach (Club item in Model)
        {
            <h2>@item.Title</h2>
            <p>@item.Description</p>
            <p>@item.ClubCategory</p>
            <a asp-controller="Club"
               asp-action="Detail"
               asp-route-id="@item.Id">
                View
            </a>
            <a>Edit</a>
        }

        <a asp-controller="Club"
           asp-action="Create">
            Create
        </a>

    </div>
</div>
